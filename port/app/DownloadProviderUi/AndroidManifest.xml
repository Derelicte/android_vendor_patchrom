        <receiver android:name=".MiuiPushReceiver">
            <intent-filter>
                <action android:name="android.intent.action.ACTION_MIUI_PUSH" />
                <action android:name="android.intent.action.DOWNLOAD_COMPLETE" />
            </intent-filter>
        </receiver>
    </application>
</manifest>
