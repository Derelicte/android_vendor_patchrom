<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:clobberSettingsKey="true">
            <key-style latin:moreKeys="@string/more_keys_for_f1" latin:backgroundType="functional" latin:keyLabelOption="hasPopupHint" latin:styleName="f1PopupStyle" />
        </case>
        <case latin:hasSettingsKey="false">
            <key-style latin:moreKeys="@string/more_keys_for_f1_settings" latin:backgroundType="functional" latin:keyLabelOption="hasPopupHint" latin:styleName="f1PopupStyle" />
        </case>
        <case latin:navigateAction="true">
            <key-style latin:moreKeys="@string/more_keys_for_f1_navigate" latin:backgroundType="functional" latin:keyLabelOption="hasPopupHint" latin:styleName="f1PopupStyle" />
        </case>
        <default>
            <key-style latin:moreKeys="@string/more_keys_for_f1" latin:backgroundType="functional" latin:keyLabelOption="hasPopupHint" latin:styleName="f1PopupStyle" />
        </default>
    </switch>
    <key-style latin:code="@integer/key_shift" latin:backgroundType="sticky" latin:keyIcon="iconShiftKey" latin:keyIconShifted="iconShiftedShiftKey" latin:styleName="shiftKeyStyle" />
    <key-style latin:code="@integer/key_delete" latin:backgroundType="functional" latin:isRepeatable="true" latin:keyIcon="iconDeleteKey" latin:styleName="deleteKeyStyle" />
    <switch>
        <case latin:mode="im">
            <key-style latin:moreKeys="@string/more_keys_for_smiley" latin:maxMoreKeysColumn="5" latin:backgroundType="functional" latin:keyOutputText=":-) " latin:keyLabel=":-)" latin:keyLabelOption="hasPopupHint" latin:styleName="returnKeyStyle" />
        </case>
        <case latin:imeAction="actionGo">
            <key-style latin:code="@integer/key_return" latin:backgroundType="action" latin:keyLabel="@string/label_go_key" latin:keyLabelOption="autoXScale" latin:styleName="returnKeyStyle" />
        </case>
        <case latin:imeAction="actionNext">
            <key-style latin:code="@integer/key_return" latin:backgroundType="action" latin:keyLabel="@string/label_next_key" latin:keyLabelOption="autoXScale" latin:styleName="returnKeyStyle" />
        </case>
        <case latin:imeAction="actionDone">
            <key-style latin:code="@integer/key_return" latin:backgroundType="action" latin:keyLabel="@string/label_done_key" latin:keyLabelOption="autoXScale" latin:styleName="returnKeyStyle" />
        </case>
        <case latin:imeAction="actionSend">
            <key-style latin:code="@integer/key_return" latin:backgroundType="action" latin:keyLabel="@string/label_send_key" latin:keyLabelOption="autoXScale" latin:styleName="returnKeyStyle" />
        </case>
        <case latin:imeAction="actionSearch">
            <key-style latin:code="@integer/key_return" latin:backgroundType="action" latin:keyIcon="iconSearchKey" latin:styleName="returnKeyStyle" />
        </case>
        <default>
            <key-style latin:code="@integer/key_return" latin:backgroundType="functional" latin:keyIcon="iconReturnKey" latin:styleName="returnKeyStyle" />
        </default>
    </switch>
    <key-style latin:code="@integer/key_space" latin:backgroundType="functional" latin:styleName="spaceKeyStyle" />
    <key-style latin:code="@integer/key_shortcut" latin:keyIcon="iconShortcutKey" latin:styleName="shortcutKeyStyle" latin:parentStyle="f1PopupStyle" />
    <key-style latin:code="@integer/key_settings" latin:backgroundType="functional" latin:keyIcon="iconSettingsKey" latin:styleName="settingsKeyStyle" />
    <key-style latin:code="@integer/key_tab" latin:backgroundType="functional" latin:keyIcon="iconTabKey" latin:keyIconPreview="iconPreviewTabKey" latin:styleName="tabKeyStyle" />
    <key-style latin:code="@integer/key_tab" latin:keyIcon="iconTabKey" latin:keyIconPreview="iconPreviewTabKey" latin:styleName="nonSpecialBackgroundTabKeyStyle" />
    <switch>
        <case latin:shortcutKeyEnabled="true" latin:hasShortcutKey="false">
            <key-style latin:code="@integer/key_switch_alpha_symbol" latin:backgroundType="functional" latin:keyLabel="@string/label_to_symbol_with_microphone_key" latin:keyLabelOption="withIconRight" latin:keyIcon="iconShortcutForLabel" latin:styleName="toSymbolKeyStyle" />
        </case>
        <default>
            <key-style latin:code="@integer/key_switch_alpha_symbol" latin:backgroundType="functional" latin:keyLabel="@string/label_to_symbol_key" latin:styleName="toSymbolKeyStyle" />
        </default>
    </switch>
    <key-style latin:code="@integer/key_switch_alpha_symbol" latin:backgroundType="functional" latin:keyLabel="@string/label_to_alpha_key" latin:styleName="toAlphaKeyStyle" />
    <key-style latin:code="@integer/key_shift" latin:backgroundType="functional" latin:keyLabel="@string/label_to_more_symbol_key" latin:styleName="toMoreSymbolKeyStyle" />
    <key-style latin:code="@integer/key_shift" latin:backgroundType="functional" latin:keyLabel="@string/label_to_symbol_key" latin:styleName="backFromMoreSymbolKeyStyle" />
    <key-style latin:moreKeys="@string/more_keys_for_punctuation" latin:maxMoreKeysColumn="@integer/mini_keyboard_column_for_punctuation" latin:backgroundType="functional" latin:keyLabel="." latin:keyHintLabel="@string/keyhintlabel_for_punctuation" latin:keyLabelOption="hasPopupHint" latin:styleName="punctuationKeyStyle" />
</merge>
