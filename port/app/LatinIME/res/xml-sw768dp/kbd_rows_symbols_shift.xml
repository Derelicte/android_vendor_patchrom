<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <include latin:keyboardLayout="@xml/kbd_key_styles" />
    <include latin:keyboardLayout="@xml/kbd_currency_key_styles" />
    <Row latin:keyWidth="8.281994%p">
        <Key latin:keyLabelOption="alignLeft" latin:keyStyle="tabKeyStyle" latin:keyWidth="7.968998%p" />
        <Key latin:keyLabel="~" />
        <Key latin:keyLabel="`" />
        <Key latin:keyLabel="|" />
        <Key latin:moreKeys="@string/more_keys_for_bullet" latin:keyLabel="•" />
        <Key latin:keyLabel="√" />
        <Key latin:moreKeys="Π" latin:keyLabel="π" />
        <Key latin:keyLabel="÷" />
        <Key latin:keyLabel="×" />
        <Key latin:moreKeys="¶" latin:keyLabel="§" />
        <Key latin:keyLabel="Δ" />
        <Key latin:keyStyle="deleteKeyStyle" latin:keyWidth="fillBoth" latin:keyXPos="-9.219003%p" />
    </Row>
    <Row latin:keyWidth="8.124995%p">
        <Key latin:keyLabelOption="alignLeft" latin:keyStyle="toAlphaKeyStyle" latin:keyWidth="11.171997%p" />
        <Key latin:keyStyle="moreCurrency1KeyStyle" />
        <Key latin:keyStyle="moreCurrency2KeyStyle" />
        <Key latin:keyStyle="moreCurrency3KeyStyle" />
        <Key latin:keyStyle="moreCurrency4KeyStyle" />
        <Key latin:moreKeys="↑,↓,←,→" latin:keyLabel="^" />
        <Key latin:moreKeys="′,″" latin:keyLabel="°" />
        <Key latin:moreKeys="∞" latin:keyLabel="±" />
        <Key latin:keyLabel="{" />
        <Key latin:keyLabel="}" />
        <Key latin:keyStyle="returnKeyStyle" latin:keyWidth="fillBoth" latin:keyXPos="-15.704%p" />
    </Row>
    <Row latin:keyWidth="8.046997%p">
        <Key latin:keyStyle="backFromMoreSymbolKeyStyle" latin:keyWidth="13.829004%p" />
        <Key latin:keyLabel="\\" />
        <Key latin:keyLabel="©" />
        <Key latin:keyLabel="®" />
        <Key latin:keyLabel="™" />
        <Key latin:keyLabel="℅" />
        <Key latin:keyLabel="[" />
        <Key latin:keyLabel="]" />
        <Key latin:keyLabel="¡" />
        <Key latin:keyLabel="¿" />
        <Key latin:keyStyle="backFromMoreSymbolKeyStyle" latin:keyWidth="fillBoth" latin:keyXPos="-13.750005%p" />
    </Row>
    <Row latin:keyWidth="8.046997%p">
        <switch>
            <case latin:hasSettingsKey="true">
                <Key latin:keyStyle="settingsKeyStyle" latin:keyWidth="8.046997%p" />
            </case>
        </switch>
        <Key latin:keyStyle="spaceKeyStyle" latin:keyWidth="37.5%p" latin:keyXPos="31.25%p" />
        <switch>
            <case latin:shortcutKeyEnabled="true">
                <Key latin:keyStyle="shortcutKeyStyle" latin:keyWidth="fillRight" latin:keyXPos="-8.046997%p" />
            </case>
        </switch>
    </Row>
</merge>
