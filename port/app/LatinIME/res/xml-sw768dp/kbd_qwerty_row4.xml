<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <Row latin:keyWidth="8.046997%p">
        <switch>
            <case latin:hasSettingsKey="true">
                <Key latin:keyStyle="settingsKeyStyle" latin:keyWidth="8.046997%p" />
            </case>
        </switch>
        <Spacer latin:keyWidth="0.0%p" latin:keyXPos="15.156996%p" />
        <switch>
            <case latin:mode="url">
                <Key latin:keyStyle="comKeyStyle" latin:keyWidth="16.084003%p" />
            </case>
            <default>
                <switch>
                    <case latin:mode="email">
                        <Key latin:keyStyle="comKeyStyle" />
                    </case>
                    <case latin:imeAction="actionSearch">
                        <Key latin:moreKeys="+" latin:keyLabel=":" latin:keyHintLabel="+" latin:keyLabelOption="hasUppercaseLetter" />
                    </case>
                    <default>
                        <Key latin:keyStyle="smileyKeyStyle" />
                    </default>
                </switch>
                <switch>
                    <case latin:mode="email">
                        <Key latin:keyLabel="\@" />
                    </case>
                    <default>
                        <Key latin:moreKeys="\@" latin:keyLabel="/" latin:keyHintLabel="\@" latin:keyLabelOption="hasUppercaseLetter" />
                    </default>
                </switch>
            </default>
        </switch>
        <Key latin:keyStyle="spaceKeyStyle" latin:keyWidth="37.5%p" latin:keyXPos="31.25%p" />
        <switch>
            <case latin:languageCode="iw">
                <include latin:keyboardLayout="@xml/kbd_row3_comma_period" />
            </case>
            <default>
                <include latin:keyboardLayout="@xml/kbd_row4_apostrophe_dash" />
            </default>
        </switch>
        <switch>
            <case latin:shortcutKeyEnabled="true">
                <Key latin:keyStyle="shortcutKeyStyle" latin:keyWidth="fillRight" latin:keyXPos="-8.046997%p" />
            </case>
        </switch>
    </Row>
</merge>
