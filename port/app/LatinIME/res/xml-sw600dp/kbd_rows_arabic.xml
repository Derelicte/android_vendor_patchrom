<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <include latin:keyboardLayout="@xml/kbd_key_styles" />
    <Row latin:keyWidth="8.000004%p">
        <Key latin:keyLabel="ض" />
        <Key latin:keyLabel="ص" />
        <Key latin:keyLabel="ث" />
        <Key latin:moreKeys="ڨ" latin:keyLabel="ق" />
        <Key latin:moreKeys="ڤ,ڢ,ڥ" latin:keyLabel="ف" />
        <Key latin:keyLabel="غ" />
        <Key latin:keyLabel="ع" />
        <Key latin:moreKeys="ﻫ|ه‍" latin:keyLabel="ه" />
        <Key latin:keyLabel="خ" />
        <Key latin:keyLabel="ح" />
        <Key latin:moreKeys="چ" latin:keyLabel="ج" />
        <Key latin:keyStyle="deleteKeyStyle" latin:keyWidth="fillBoth" latin:keyXPos="-10.000002%p" />
    </Row>
    <Row latin:keyWidth="8.000004%p">
        <Key latin:moreKeys="ڜ" latin:keyLabel="ش" latin:keyXPos="2.9999971%p" />
        <Key latin:keyLabel="س" />
        <Key latin:moreKeys="ئ,ى" latin:keyLabel="ي" />
        <Key latin:moreKeys="پ" latin:keyLabel="ب" />
        <Key latin:moreKeys="ﻻ|لا,ﻷ|لأ,ﻹ|لإ,ﻵ|لآ" latin:keyLabel="ل" />
        <Key latin:moreKeys="ء,ٱ,أ,إ,آ" latin:keyLabel="ا" />
        <Key latin:keyLabel="ت" />
        <Key latin:keyLabel="ن" />
        <Key latin:keyLabel="م" />
        <Key latin:moreKeys="گ,ک" latin:keyLabel="ك" />
        <Key latin:keyLabel="ط" />
        <Key latin:keyStyle="returnKeyStyle" latin:keyWidth="fillBoth" latin:keyXPos="-14.600002%p" />
    </Row>
    <Row latin:keyWidth="8.000004%p">
        <switch>
            <case latin:mode="email">
                <Key latin:keyLabel="\@" />
            </case>
            <case latin:mode="url">
                <Key latin:moreKeys="_" latin:keyLabel="-" latin:keyHintLabel="_" latin:keyLabelOption="hasUppercaseLetter" />
            </case>
            <case latin:imeAction="actionSearch">
                <Key latin:moreKeys="+" latin:keyLabel=":" latin:keyHintLabel="+" latin:keyLabelOption="hasUppercaseLetter" />
            </case>
            <default>
                <Key latin:keyStyle="smileyKeyStyle" />
            </default>
        </switch>
        <Key latin:keyLabel="ئ" />
        <Key latin:keyLabel="ء" />
        <Key latin:keyLabel="ؤ" />
        <Key latin:keyLabel="ر" />
        <Key latin:keyLabel="ذ" />
        <Key latin:keyLabel="ى" />
        <Key latin:keyLabel="ة" />
        <Key latin:keyLabel="و" />
        <Key latin:moreKeys="ژ" latin:keyLabel="ز" />
        <Key latin:keyLabel="ظ" />
        <Key latin:keyLabel="د" />
    </Row>
    <include latin:keyboardLayout="@xml/kbd_qwerty_row4" />
</merge>
