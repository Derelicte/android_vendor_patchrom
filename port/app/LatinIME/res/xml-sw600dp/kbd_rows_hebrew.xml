<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <include latin:keyboardLayout="@xml/kbd_key_styles" />
    <Row latin:keyWidth="9.000004%p">
        <include latin:keyboardLayout="@xml/kbd_row4_apostrophe_dash" />
        <Key latin:keyLabel="ק" />
        <Key latin:keyLabel="ר" />
        <Key latin:keyLabel="א" />
        <Key latin:keyLabel="ט" />
        <Key latin:keyLabel="ו" />
        <Key latin:keyLabel="ן" />
        <Key latin:keyLabel="ם" />
        <Key latin:keyLabel="פ" />
        <Key latin:keyStyle="deleteKeyStyle" latin:keyWidth="fillBoth" latin:keyXPos="-12.0%p" />
    </Row>
    <Row latin:keyWidth="9.000004%p">
        <Key latin:keyLabel="ש" latin:keyXPos="4.4999957%p" />
        <Key latin:keyLabel="ד" />
        <Key latin:moreKeys="ג׳" latin:keyLabel="ג" />
        <Key latin:keyLabel="כ" />
        <Key latin:keyLabel="ע" />
        <Key latin:moreKeys="ײַ" latin:keyLabel="י" />
        <Key latin:moreKeys="ח׳" latin:keyLabel="ח" />
        <Key latin:keyLabel="ל" />
        <Key latin:keyLabel="ך" />
        <Key latin:keyLabel="ף" />
    </Row>
    <Row latin:keyWidth="8.899999%p">
        <switch>
            <case latin:mode="email">
                <Key latin:keyLabel="\@" latin:keyWidth="10.000002%p" />
            </case>
            <case latin:mode="url">
                <Key latin:moreKeys="_" latin:keyLabel="-" latin:keyHintLabel="_" latin:keyLabelOption="hasUppercaseLetter" latin:keyWidth="10.000002%p" />
            </case>
            <case latin:imeAction="actionSearch">
                <Key latin:moreKeys="+" latin:keyLabel=":" latin:keyHintLabel="+" latin:keyLabelOption="hasUppercaseLetter" latin:keyWidth="10.000002%p" />
            </case>
            <default>
                <Key latin:keyStyle="smileyKeyStyle" latin:keyWidth="10.000002%p" />
            </default>
        </switch>
        <Key latin:moreKeys="ז׳" latin:keyLabel="ז" />
        <Key latin:keyLabel="ס" />
        <Key latin:keyLabel="ב" />
        <Key latin:keyLabel="ה" />
        <Key latin:keyLabel="נ" />
        <Key latin:keyLabel="מ" />
        <Key latin:moreKeys="צ׳" latin:keyLabel="צ" />
        <Key latin:moreKeys="ת׳" latin:keyLabel="ת" />
        <Key latin:moreKeys="ץ׳" latin:keyLabel="ץ" />
        <Key latin:keyStyle="returnKeyStyle" latin:keyWidth="fillBoth" latin:keyXPos="-10.399998%p" />
    </Row>
    <include latin:keyboardLayout="@xml/kbd_qwerty_row4" />
</merge>
