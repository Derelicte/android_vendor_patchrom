<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <Row latin:keyWidth="8.899999%p">
        <Key latin:keyStyle="toSymbolKeyStyle" latin:keyWidth="13.0%p" />
        <Key latin:keyStyle="tabKeyStyle" />
        <switch>
            <case latin:mode="email">
                <Key latin:keyStyle="comKeyStyle" />
            </case>
            <case latin:mode="url">
                <Key latin:keyStyle="comKeyStyle" />
            </case>
            <default>
                <Key latin:moreKeys="\@" latin:keyLabel="/" latin:keyHintLabel="\@" latin:keyLabelOption="hasUppercaseLetter" />
            </default>
        </switch>
        <Key latin:keyStyle="spaceKeyStyle" latin:keyWidth="39.750004%p" latin:keyXPos="30.75%p" />
        <switch>
            <case latin:languageCode="iw">
                <include latin:keyboardLayout="@xml/kbd_row3_comma_period" />
            </case>
            <default>
                <include latin:keyboardLayout="@xml/kbd_row4_apostrophe_dash" />
            </default>
        </switch>
        <Spacer latin:keyWidth="0.0%p" latin:keyXPos="-10.000002%p" />
        <include latin:keyboardLayout="@xml/kbd_qwerty_f2" />
    </Row>
</merge>
