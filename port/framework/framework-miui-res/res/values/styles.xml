<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Widget.ListView.NoDivider" parent="@android:style/Widget.ListView">
        <item name="android:fadingEdge">none</item>
        <item name="android:listSelector">@null</item>
        <item name="android:cacheColorHint">@null</item>
        <item name="android:divider">@null</item>
        <item name="android:dividerHeight">0.0dip</item>
    </style>
    <style name="Widget.TabWidget.Secondary" parent="@android:style/Widget.TabWidget">
        <item name="android:textColor">@color/secondary_tab_text</item>
        <item name="android:shadowColor">#e6ffffff</item>
    </style>
    <style name="Widget.Button" parent="@android:style/Widget">
        <item name="android:textAppearance">?android:textAppearanceSmallInverse</item>
        <item name="android:textColor">@color/button_text</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@android:drawable/btn_default</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:shadowColor">@color/button_text_shadow</item>
        <item name="android:shadowDy">@integer/button_text_shadow_dy</item>
        <item name="android:shadowRadius">0.2</item>
    </style>
    <style name="Widget.Button.Bar" parent="@style/Widget.Button">
        <item name="android:textSize">@dimen/button_bar_text_size</item>
        <item name="android:background">@drawable/btn_bar</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="BottomButtonBar">
        <item name="android:textSize">@dimen/button_bar_text_size</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">bottom</item>
        <item name="android:background">@drawable/bottom_button_bar</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="TextAppearanceSecondaryTab">
        <item name="android:textSize">15.0sp</item>
        <item name="android:textColor">@color/secondary_tab_text</item>
    </style>
    <style name="TextAppearanceSecondaryTab.Left" parent="@style/TextAppearanceSecondaryTab">
        <item name="android:background">@drawable/secondary_tab_left</item>
    </style>
    <style name="TextAppearanceSecondaryTab.Middle" parent="@style/TextAppearanceSecondaryTab">
        <item name="android:background">@drawable/secondary_tab_middle</item>
    </style>
    <style name="TextAppearanceSecondaryTab.Right" parent="@style/TextAppearanceSecondaryTab">
        <item name="android:background">@drawable/secondary_tab_right</item>
    </style>
    <style name="TextViewFloatPanelButton">
        <item name="android:textAppearance">?android:textAppearanceSmall</item>
        <item name="android:textColor">@color/textview_floatpanel_button_text_color</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/textview_floatpanel_button_bg</item>
        <item name="android:layout_width">60.0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">38.0dip</item>
        <item name="android:singleLine">true</item>
        <item name="android:shadowColor">#ccffffff</item>
        <item name="android:shadowDy">1.0</item>
        <item name="android:shadowRadius">0.2</item>
    </style>
    <style name="PreviewInMultipleItem">
        <item name="android:focusable">false</item>
        <item name="android:layout_width">81.599976dip</item>
        <item name="android:layout_height">136.0dip</item>
        <item name="android:layout_marginLeft">1.0px</item>
        <item name="android:scaleType">centerCrop</item>
    </style>
    <style name="PreviewInCombineTextItem">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">5.0dip</item>
        <item name="android:layout_marginRight">5.0dip</item>
    </style>
    <style name="PreviewInCombineTextItemImage">
        <item name="android:background">@drawable/thumbnail_frame</item>
        <item name="android:layout_width">92.0dip</item>
        <item name="android:layout_height">144.0dip</item>
    </style>
    <style name="PreviewInCombineTextItemText">
        <item name="android:layout_width">92.0dip</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="PreviewInCombineThinItem">
        <item name="android:background">@drawable/thumbnail_frame</item>
        <item name="android:layout_width">92.0dip</item>
        <item name="android:layout_height">144.0dip</item>
        <item name="android:layout_marginLeft">5.0dip</item>
        <item name="android:layout_marginRight">5.0dip</item>
    </style>
    <style name="PreviewInCombineItem">
        <item name="android:background">@drawable/thumbnail_frame</item>
        <item name="android:layout_width">92.0dip</item>
        <item name="android:layout_height">83.0dip</item>
        <item name="android:layout_marginLeft">2.0dip</item>
        <item name="android:layout_marginRight">2.0dip</item>
    </style>
    <style name="PreviewItemImage">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:scaleType">centerCrop</item>
        <item name="android:adjustViewBounds">true</item>
    </style>
    <style name="ResourceProperty">
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="ResourcePropertyTitle">
        <item name="android:textAppearance">?android:textAppearanceSmall</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:gravity">right</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="ResourcePropertyContent">
        <item name="android:textAppearance">?android:textAppearanceSmall</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:gravity">left</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10.0dip</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_weight">2.0</item>
    </style>
    <style name="Theme" parent="@android:style/Theme.Holo" />
    <style name="Theme.Light" parent="@android:style/Theme.Holo.Light">
        <item name="android:actionBarStyle">@style/Widget.Holo.Light.ActionBar.Solid.HomeUp</item>
        <item name="android:actionOverflowButtonStyle">@style/Widget.Holo.Light.ActionButton.Overflow</item>
        <item name="android:homeAsUpIndicator">@drawable/ab_home_up</item>
    </style>
    <style name="Widget.Holo.Light.ActionBar.Solid.HomeUp" parent="@android:style/Widget.Holo.Light.ActionBar.Solid">
        <item name="android:logo">@drawable/ab_home_up_divider</item>
        <item name="android:displayOptions">useLogo|showHome|homeAsUp|showTitle</item>
    </style>
    <style name="EditorCustomActionButton" parent="@android:style/Widget.Holo.Light.ActionButton">
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@drawable/custom_action_button</item>
        <item name="android:focusable">true</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="EditorCustomActionButtonImage">
        <item name="android:padding">4.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="EditorCustomActionButtonText">
        <item name="android:textAppearance">?android:textAppearanceMedium</item>
        <item name="android:textColor">#b2000000</item>
        <item name="android:ellipsize">none</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:padding">4.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="EditorCustomActionBarTheme" parent="@android:style/Theme.Holo">
        <item name="android:background">@drawable/custom_action_bar_bg</item>
    </style>
    <style name="TextAppearance.ListView" parent="@android:attr/textAppearance">
        <item name="android:shadowDx">0.0</item>
