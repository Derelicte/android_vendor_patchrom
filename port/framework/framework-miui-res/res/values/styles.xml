        <item name="android:textAppearanceListItem">@android:style/TextAppearance.Large.Inverse</item>
        <item name="android:textAppearanceListItemSmall">@android:style/TextAppearance.Large.Inverse</item>
    </style>
    <style name="Widget.Button.Transparent" parent="@android:style/Widget.Button">
        <item name="android:textAppearance">?android:textAppearanceSmall</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:background">@drawable/android_btn_default_transparent</item>
