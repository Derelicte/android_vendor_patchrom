    <style name="TextAppearance.ListView.SecondaryItem" parent="@style/TextAppearance.ListView">
        <item name="android:textSize">@dimen/list_secondary_text_size</item>
        <item name="android:textColor">@color/secondary_text</item>
        <item name="android:shadowColor">@color/secondary_text_color_shadow</item>
