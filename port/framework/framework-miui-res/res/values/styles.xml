    <style name="Theme.Panel.Volume" parent="@android:style/Theme.Panel">
        <item name="android:windowAnimationStyle">@style/Animation.VolumePanel</item>
        <item name="android:windowCloseOnTouchOutside">true</item>
    </style>
    <style name="Animation.VolumePanel" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@null</item>
        <item name="android:windowExitAnimation">@android:anim/fade_out</item>
    </style>
    <style name="SlidingButtonStyle">
        <item name="buttonFrame">@drawable/sliding_btn_frame</item>
        <item name="buttonSlider">@drawable/sliding_btn_slider</item>
        <item name="buttonSliderPressed">@drawable/sliding_btn_slider_pressed</item>
        <item name="buttonOnDisable">@drawable/sliding_btn_on_disable</item>
        <item name="buttonOffDisable">@drawable/sliding_btn_off_disable</item>
        <item name="buttonMask">@drawable/sliding_btn_mask</item>
        <item name="buttonBar">@drawable/sliding_btn_bar</item>
    </style>
</resources>
