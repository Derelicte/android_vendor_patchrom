<?xml version="1.0" encoding="utf-8"?>
<!-- Formatting note: terminate all comments with a period, to avoid breaking
     the documentation output. To suppress comment lines from the documentation
     output, insert an eat-comment element after the comment lines.
-->

<resources>
      <!-- @hide -->
    <declare-styleable name="SpectrumVisualizer">
      <!-- @hide -->
      <attr name="symmetry" format="boolean"/>
      <!-- @hide -->
      <attr name="sliding_dot_bar" format="reference"/>
      <!-- @hide -->
      <attr name="sliding_shadow_dot_bar" format="reference"/>
      <!-- @hide -->
      <attr name="sliding_panel" format="reference"/>
      <!-- @hide -->
      <attr name="alpha_width" format="integer"/>
      <!-- @hide -->
      <attr name="update_enable" format="boolean"/>
      <!-- @hide -->
      <attr name="care_streamactive" format="boolean"/>
   </declare-styleable>
    <!-- =============================== -->
    <!-- MiuiInCallAnswerWidgetHorizontal class attributes -->
    <!-- =============================== -->
    <declare-styleable name="MiuiInCallAnswerWidgetHorizontal">
        <!-- Reference to an array resource with light that be shown as targets around a circle.-->
        <attr name="targetDrawablesLight" format="reference"/>
        <attr name="rightHintAnimationDrawable" format="reference"/>
    </declare-styleable>

    <!-- =============================== -->
    <!-- SlidingButton attributes        -->
    <!-- =============================== -->
    <declare-styleable name="SlidingButton">
        <attr name="buttonFrame" format="reference"/>
        <attr name="buttonSlider" format="reference"/>
        <attr name="buttonSliderPressed" format="reference"/>
        <attr name="buttonOnDisable" format="reference"/>
        <attr name="buttonOffDisable" format="reference"/>
        <attr name="buttonMask" format="reference"/>
        <attr name="buttonBar" format="reference"/>
    </declare-styleable>
</resources>
