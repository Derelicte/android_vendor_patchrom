        <item>Cool</item>                    <!-- 7: B-) -->
        <item>Money mouth</item>             <!-- 8: :-$ -->
        <item>Foot in mouth</item>           <!-- 9: :-! -->
        <item>Embarrassed</item>             <!-- 10: :-[ -->
        <item>Angel</item>                   <!-- 11: O:-) -->
        <item>Undecided</item>               <!-- 12: :-\\ -->
        <item>Crying</item>                  <!-- 13: :\'( -->
        <item>Lips are sealed</item>         <!-- 14: :-X -->
        <item>Laughing</item>                <!-- 15: :-D -->
        <item>Confused</item>                <!-- 16: o_O -->
