<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- answer widget setting entries -->
    <string-array translatable="false" name="answer_widget_entries_miui">
        <item>@string/answer_widget_sliding_vertical_miui</item>
        <item>@string/answer_widget_sliding_horizontal_miui</item>
    </string-array>
    <!-- answer widget setting values: 0 - vertical, 1 - horizontal -->
    <string-array translatable="false" name="answer_widget_entry_values_miui">
        <item>0</item>
        <item>1</item>
    </string-array>

    <!-- vibrate settings for connect/disconnect -->
    <string-array translatable="false" name="connect_disconnect_vibrate_entries_miui">
        <item>@string/connect_disconnect_vibrate_normal_miui</item>
        <item>@string/connect_disconnect_vibrate_weak_miui</item>
        <item>@string/connect_disconnect_vibrate_none_miui</item>
    </string-array>
    <!-- vibrate length for connection/disconnect (ms) -->
    <string-array translatable="false" name="connect_disconnect_vibrate_entry_values_miui">
        <item>100</item>
        <item>50</item>
        <item>0</item>
    </string-array>

    <!-- voice quality setting entries -->
    <string-array translatable="false" name="voice_quality_ui_values_miui">
        <item>@string/voice_quality_ui_values_normal_miui</item>
        <item>@string/voice_quality_ui_values_clear_miui</item>
        <item>@string/voice_quality_ui_values_crisp_miui</item>
        <item>@string/voice_quality_ui_values_bright_miui</item>
    </string-array>

    <!-- cdma call waiting entries -->
    <string-array translatable="false" name="cdma_call_waiting_entries_miui">
        <item>@string/enable_miui</item>
        <item>@string/disable_miui</item>
    </string-array>
    <!-- cdma call waiting values: 1 - enable, 0 - disable -->
    <string-array translatable="false" name="cdma_call_waiting_entry_values_miui">
        <item>1</item>
        <item>0</item>
    </string-array>

    <!-- For most incoming calls the MultiWaveView widget provides 3 choices:
         - Answer (drag right)
         - Reject (drag left)
         - Respond via SMS (drag up) -->
    <array translatable="false" name="in_call_answer_widget_3way_targets_miui">
        <item>@drawable/ic_in_call_answer_miui</item>
        <item>@drawable/ic_in_call_send_sms_miui</item>
        <item>@drawable/ic_in_call_decline_miui</item>
        <item>@null</item>"
    </array>
    <array translatable="false" name="in_call_answer_widget_3way_targets_light_miui">
        <item>@drawable/ic_in_call_answer_light_miui</item>
        <item>@drawable/ic_in_call_send_sms_light_miui</item>
        <item>@drawable/ic_in_call_decline_light_miui</item>
        <item>@null</item>"
    </array>
    <array translatable="false" name="in_call_answer_widget_3way_target_descriptions_miui">
        <item>@string/description_target_answer_miui</item>
        <item>@string/description_target_send_sms_miui</item>
        <item>@string/description_target_decline_miui</item>
        <item>@null</item>"
    </array>
