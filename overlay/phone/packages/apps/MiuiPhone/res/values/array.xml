        <item>@string/description_direction_right_miui</item>
        <item>@string/description_direction_up_miui</item>
        <item>@string/description_direction_left_miui</item>
        <item>@null</item>
    </array>

    <!-- But in some cases "Respond via SMS" isn't available, so there are
         only 2 choices:
         - Answer (drag right)
         - Reject (drag left) -->
    <array translatable="false" name="in_call_answer_widget_2way_targets_miui">
        <item>@drawable/ic_in_call_answer_miui</item>
        <item>@null</item>
        <item>@drawable/ic_in_call_decline_miui</item>
        <item>@null</item>"
