<!-- Do not translate strings of this xml -->
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- This is the separator character used when building the string that shows number of albums and songs. -->
    <string name="albumsongseparator" translatable="false">\n</string>

    <!-- Used for the 2nd and 3rd line (artist and album) in the notification area item for the music app -->
    <string name="notification_artist_album" translatable="false"><xliff:g id="album">%2$s</xliff:g>\n<xliff:g id="artist">%1$s</xliff:g></string>

    <!--
    This string is used as the format string in a String.format call,
    and 5 additional arguments are available for printing:
    1 - hours
    2 - minutes
    3 - minutes%60
    4 - seconds
    5 - second%60
    -->
    <skip />
    <!-- Do not translate. Duration format for duration < 1 hour -->
    <string name="durationformatshort" translatable="false"><xliff:g id="format">%2$d:%5$02d</xliff:g></string>
    <!-- Do not translate. Duration format for duration >= 1 hour -->
    <string name="durationformatlong" translatable="false"><xliff:g id="format">%1$d:%3$02d:%5$02d</xliff:g></string>
    <string name="durationformatshort_padding_with_0" translatable="false" ><xliff:g id="format">%2$02d:%5$02d</xliff:g></string>

    <!-- sleep mode -->
    <string name="min_interval" translatable="false">1</string>
    <string name="max_interval" translatable="false">90</string>
    <string name="default_interval" translatable="false">30</string>

    <string name="folder_title_format" translatable="false">%s(%d)</string>

    <string name="filter_max" translatable="false">2MB</string>
    <string name="filter_min" translatable="false">0K</string>

    <!-- alphabet fast indexer -->
    <string-array name="alphabet_table" translatable="false">
        <item>#</item>
        <item>A</item>
        <item>B</item>
        <item>C</item>
        <item>D</item>
        <item>E</item>
        <item>F</item>
        <item>G</item>
        <item>H</item>
        <item>I</item>
        <item>J</item>
        <item>K</item>
        <item>L</item>
        <item>M</item>
        <item>N</item>
        <item>O</item>
        <item>P</item>
        <item>Q</item>
        <item>R</item>
        <item>S</item>
        <item>T</item>
        <item>U</item>
        <item>V</item>
        <item>W</item>
        <item>X</item>
        <item>Y</item>
        <item>Z</item>
    </string-array>

    <!-- The fast scroll thumb will show one character from this string depending on the position in the list -->
    <string name="fast_scroll_alphabet" translatable="false">\u0020ABCDEFGHIJKLMNOPQRSTUVWXYZ</string>

</resources>